{
  "components": {
    "schemas": {
      "Error": {
        "properties": {
          "message": {
            "example": "An error occurred while processing your request",
            "type": "string"
          }
        },
        "required": [
          "message"
        ],
        "type": "object"
      }
    },
    "securitySchemes": {
      "ApiKeyAuth": {
        "in": "header",
        "name": "x-rapidapi-key",
        "type": "apiKey"
      }
    }
  },
  "info": {
    "description": "Fetches LinkedIn-style company data using a domain name. Useful when you want structured metadata about a company based on its website domain. \u26a0\ufe0f Charges 1 credit per call.",
    "title": "LinkedIn API",
    "version": "1.0.0",
    "x-logo": {
      "url": "data:image/svg+xml;base64,"
    },
    "x-bionic-slug": "linkedin"
  },
  "openapi": "3.0.3",
  "paths": {
    "/get-company-by-domain": {
      "get": {
        "description": "Fetches LinkedIn-style company data (name, industry, size, website, logo, LinkedIn URL, etc.) using the company's domain. Useful when you have a domain (e.g., example.com) and want structured company metadata.",
        "operationId": "getCompanyByDomain",
        "parameters": [
          {
            "description": "The domain associated with the company (e.g., example.com)",
            "in": "query",
            "name": "domain",
            "required": true,
            "schema": {
              "format": "hostname",
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "data": {
                      "properties": {
                        "company_name": {
                          "example": "Example Inc.",
                          "type": "string"
                        },
                        "domain": {
                          "example": "example.com",
                          "type": "string"
                        },
                        "employee_count": {
                          "example": "201-500",
                          "type": "string"
                        },
                        "hq_city": {
                          "example": "San Francisco",
                          "type": "string"
                        },
                        "hq_country": {
                          "example": "USA",
                          "type": "string"
                        },
                        "industry": {
                          "example": "Software Development",
                          "type": "string"
                        },
                        "linkedin_url": {
                          "example": "https://www.linkedin.com/company/example/",
                          "format": "uri",
                          "type": "string"
                        },
                        "logo_url": {
                          "example": "https://logo.example.com/image.png",
                          "format": "uri",
                          "type": "string"
                        },
                        "website": {
                          "example": "https://example.com",
                          "format": "uri",
                          "type": "string"
                        },
                        "year_founded": {
                          "example": 2005,
                          "type": "integer"
                        }
                      },
                      "type": "object"
                    },
                    "message": {
                      "example": "Request successful.",
                      "type": "string"
                    }
                  },
                  "required": [
                    "message",
                    "data"
                  ],
                  "type": "object"
                }
              }
            },
            "description": "Company data retrieved successfully"
          },
          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            },
            "description": "Invalid request"
          },
          "401": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            },
            "description": "Unauthorized - missing or invalid API key"
          },
          "404": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            },
            "description": "Company not found for the provided domain"
          },
          "500": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            },
            "description": "Internal server error"
          }
        },
        "security": [
          {
            "ApiKeyAuth": []
          }
        ],
        "summary": "Get company details by domain"
      }
    }
  },
  "servers": [
    {
      "url": "https://fresh-linkedin-profile-data.p.rapidapi.com"
    }
  ]
}
