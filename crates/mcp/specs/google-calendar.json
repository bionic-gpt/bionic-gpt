{
  "components": {
    "schemas": {
      "Event": {
        "properties": {
          "attendees": {
            "items": {
              "properties": {
                "email": {
                  "example": "alice@example.com",
                  "format": "email",
                  "type": "string"
                }
              },
              "type": "object"
            },
            "type": "array"
          },
          "description": {
            "example": "Weekly sync-up meeting",
            "type": "string"
          },
          "end": {
            "properties": {
              "dateTime": {
                "example": "2025-06-08T11:00:00-07:00",
                "format": "date-time",
                "type": "string"
              },
              "timeZone": {
                "example": "America/Los_Angeles",
                "type": "string"
              }
            },
            "required": [
              "dateTime"
            ],
            "type": "object"
          },
          "location": {
            "example": "Zoom",
            "type": "string"
          },
          "start": {
            "properties": {
              "dateTime": {
                "example": "2025-06-08T10:00:00-07:00",
                "format": "date-time",
                "type": "string"
              },
              "timeZone": {
                "example": "America/Los_Angeles",
                "type": "string"
              }
            },
            "required": [
              "dateTime"
            ],
            "type": "object"
          },
          "summary": {
            "example": "Project Sync",
            "type": "string"
          }
        },
        "required": [
          "summary",
          "start",
          "end"
        ],
        "type": "object"
      }
    },
    "securitySchemes": {
      "OAuth2": {
        "flows": {
          "authorizationCode": {
            "authorizationUrl": "https://accounts.google.com/o/oauth2/v2/auth",
            "scopes": {
              "https://www.googleapis.com/auth/calendar": "Read/write access to a user's calendar"
            },
            "tokenUrl": "https://oauth2.googleapis.com/token"
          }
        },
        "type": "oauth2"
      }
    }
  },
  "info": {
    "description": "Minimal OpenAPI spec for creating events in Google Calendar using OAuth 2.0",
    "title": "Google Calendar API - Create Events",
    "version": "1.0.0",
    "x-logo": {
      "url": "data:image/svg+xml;base64,"
    },
    "x-bionic-slug": "gcal"
  },
  "openapi": "3.0.3",
  "paths": {
    "/calendars/{calendarId}/events": {
      "post": {
        "operationId": "createEvent",
        "parameters": [
          {
            "description": "Calendar identifier, e.g. 'primary'",
            "in": "path",
            "name": "calendarId",
            "required": true,
            "schema": {
              "example": "primary",
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Event"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Event"
                }
              }
            },
            "description": "Event successfully created"
          }
        },
        "security": [
          {
            "OAuth2": [
              "https://accounts.google.com/o/oauth2/v2/auth"
            ]
          }
        ],
        "summary": "Create a calendar event"
      }
    }
  },
  "servers": [
    {
      "url": "https://www.googleapis.com/calendar/v3"
    }
  ]
}
