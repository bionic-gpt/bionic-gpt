{
  "name": "bionic-pipeline",
  "sdk": "rust",
  "source": "./crates/dagger-pipeline",
  "actions": {
    "pull-request": {
      "command": "cargo run -p dagger-pipeline --release -- pull-request"
    },
    "all": {
      "command": "cargo run -p dagger-pipeline --release -- all"
    },
    "build-cli-linux": {
      "command": "cargo run -p dagger-pipeline --release -- build-cli linux"
    },
    "build-cli-macos": {
      "command": "cargo run -p dagger-pipeline --release -- build-cli macos"
    },
    "build-cli-windows": {
      "command": "cargo run -p dagger-pipeline --release -- build-cli windows"
    }
  }
}
